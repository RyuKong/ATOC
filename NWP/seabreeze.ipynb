    "# print(numpy.shape(TSK))\n",
    "\n",
    "TSK = df['TSK']\n",
    "\n",
    "# You can change these cells to see if the temperature differences are symmetric on the left and \n",
    "# on the right side of the piece of land.\n",
    "\n",
    "land_cell = 100 # index in the x-direction\n",
    "water_cell = 50 # index in the x-direction\n",
